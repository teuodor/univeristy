def consistent(l):
    if not l[0] == 0 and l[len(l)-1] == 0:
        return False
    i=2
    while i<len(l) and (l[i]-l[i-1] == 1 or l[i]-l[i-1] == -1):
        i=i+1
    if i == len(l)-1:
        return True
    return False
def solution(l,n):
    if len(l) == 2*n+1:
        return True
    return False
