from cmath import sqrt
def prim(n):
    if n == 0 or n == 1:
        return False
    elif n == 2:
        return True
    elif n%2 == 0:
        return False
    else:
        for i in range(0,sqrt(n)):
            
def solution(l,s):
    suma = 0
    for i in l:
        suma += i
    if suma == s:
        return True
    return False
def consistent(l,s):
    suma = 0
    for i in l:
        suma += i
    if suma <= s:
        return True
    return False
